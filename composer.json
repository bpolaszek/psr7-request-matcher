{
  "name": "bentools/psr7-request-matcher",
  "description": "A PSR-7 RequestMatcher interface for use into several projects.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "bpolaszek",
      "email": "bpolaszek@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "php": ">=5.4",
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "satooshi/php-coveralls": "@stable",
    "guzzlehttp/psr7": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "BenTools\\Psr7\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\Psr7\\RequestMatcher\\Tests\\": "tests"
    }
  }
}
